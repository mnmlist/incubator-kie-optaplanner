<?xml version="1.0" encoding="utf-8"?>
<plannerBenchmark xmlns="https://www.optaplanner.org/xsd/benchmark">
  <benchmarkDirectory>data</benchmarkDirectory>
  <inheritedSolverBenchmark>
    <solver>
      <termination>
        <minutesSpentLimit>5</minutesSpentLimit>
      </termination>
    </solver>
    <problemBenchmarks>
      <problemStatisticType>BEST_SCORE</problemStatisticType>
    </problemBenchmarks>
  </inheritedSolverBenchmark>
  <solverBenchmark>
    <name>Entity Tabu Search</name>
    <solver>
      <solutionClass>com.sankuai.optaplanner.core.impl.testdata.domain.TestdataSolution</solutionClass>
      <entityClass>com.sankuai.optaplanner.core.impl.testdata.domain.TestdataEntity</entityClass>
    </solver>
    <problemBenchmarks>
      <solutionFileIOClass>com.sankuai.optaplanner.benchmark.config.PlannerBenchmarkConfigTest$TestdataSolutionFileIO</solutionFileIOClass>
      <inputSolutionFile>nonExistingDataset1.xml</inputSolutionFile>
    </problemBenchmarks>
  </solverBenchmark>
</plannerBenchmark>
